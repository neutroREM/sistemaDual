@model IEnumerable<sistemaDual.Models.Domicilio>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Direccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Colonia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Municipio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodigoPostal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Otros)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AlumnoDual)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Universidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Empresa)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Colonia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Municipio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodigoPostal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Otros)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AlumnoDual.AlumnoDualID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Universidad.UniversidadID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Empresa.EmpresaID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DomicilioID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DomicilioID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DomicilioID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
